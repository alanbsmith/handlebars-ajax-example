<div class='panel panel-default'>
  <div class='panel-heading'>
    <h3>{{ title }}</h3>
  </div>
  <div class='panel-body'>
    <ul class='pizzeria-list'>
      {{#each pizzerias}}
        <li>
          <a href="{{properties.website}}">{{properties.pizzeria}}</a>
          <p>{{properties.address}} {{properties.city}}</p>
        </li>
      {{/each}}
    </ul>
  </div>
</div>
